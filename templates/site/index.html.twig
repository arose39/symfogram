{% extends 'base.html.twig' %}

{% block title %}Hello SiteController!{% endblock %}

{% block body %}
    <ul>
        {% for user in users %}
            <li>
                <a href="{{ path('profile', {'nickname':user.nickname}) }}">
                    {{ user.firstName }}  {{ user.lastName }}
                </a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
